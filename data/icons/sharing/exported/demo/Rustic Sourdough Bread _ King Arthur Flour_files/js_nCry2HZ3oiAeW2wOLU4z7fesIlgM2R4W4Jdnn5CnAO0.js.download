(function ($, Drupal, drupalSettings) {
  'use strict';

  Drupal.recipeBox = Drupal.recipeBox || {};
  Drupal.recipeBox.apiBaseUrl = Drupal.recipeBox.apiBaseUrl || drupalSettings.kaf_recipe_box.api_base_url;
  Drupal.recipeBox.shopSigninUrl = Drupal.recipeBox.shopSigninUrl || drupalSettings.kaf_recipe_box.shop_signin_url || '/shop/signin';
  Drupal.recipeBox.getEntitiesUrl = Drupal.recipeBox.getEntitiesUrl || drupalSettings.kaf_recipe_box.get_entities_url || '/recipebox/get';

  /**
   * RecipeBox theme functions.
   */

  Drupal.theme.kafRecipeBoxList = function () {
    return '<ul class="recipe-box js-recipe-box"></ul>';
  };

  Drupal.theme.kafRecipeBoxListItem = function (recipe) {
    var build = {};
    build.id = recipe.uuid;
    build.title = '<div class="recipe-box__title-buttons"><h4>' + recipe.title + '</h4> <a href="' + recipe.url + '" class="btn recipe-box__button--view">' + Drupal.t('View') + '</a> <a href="' + recipe.url + '?action=print" class="btn recipe-box__button--print">' + Drupal.t('Print') + '</a></div>';
    build.thumbnail = '<img src="' + recipe.thumbnail + '" alt="' + recipe.title + '" />';
    build.removeLink = '<div class="recipe-box__button-wrapper"><a href="#" class="btn btn--secondary recipe-box__button--remove js-recipe-box-button-remove" data-recipe-uuid="' + recipe.uuid + '">' +
      Drupal.t('Remove Recipe') +
      '<span data-recipebox-message-row-id="' + recipe.uuid + '">' +
      '</a></div>';

    return '<li class="recipe-box-row" data-recipe-box-row-id="' + build.id + '">' +
        build.thumbnail + ' ' +
        build.title + ' ' +
        build.removeLink + ' ' +
      '</li>';
  };

  /**
   * Fetch Recipe Box entities and activate Remove button.
   */
  Drupal.recipeBox.getRecipeBoxItems = function (uuids, recipeList) {
    $.ajax({
      type: "POST",
      url: Drupal.recipeBox.getEntitiesUrl,
      data: { rid: uuids },
      xhrFields: { withCredentials: true },
      success: function (response) {
        if (response.data.length > 0) {
          var recipes = response.data;
          recipes.sort(function(a, b) {
            return a.title > b.title ? 1 : -1;
          });

          $.each(recipes, function (index, recipe) {
            $(recipeList).append(Drupal.theme('kafRecipeBoxListItem', recipe));
          });

          Drupal.recipeBox.activateRemoveButtons(recipeList);
        }
      },
      error: function (response, textStatus, errorThrown) {
        console.error('getRecipeBoxItems returned error: ' + errorThrown);
      }
    });
  }

  /**
   * Add recipe to Recipe Box.
   */
  Drupal.recipeBox.addRecipeToBox = function (triggeringElement, recipeId) {
    var $button = $(triggeringElement);
    // progress indicator
    $button.addClass('btn--throbber');

    var uuid = $button.data('recipe-uuid');

    $.ajax({
        type: "POST",
        url: Drupal.recipeBox.apiBaseUrl,
        data: { rid: uuid },
        xhrFields: { withCredentials: true },
        success: function (response) {
          if (response[0] == true) {
            $button.removeClass('btn--throbber')
              .addClass('btn--success')
              .html(Drupal.t('Recipe Saved'));
          }
          else {
            console.error('addRecipeToBox an error occurred');
            // Redirect to login.
            var destination = window.location;
            window.location = Drupal.recipeBox.shopSigninUrl + '?forward-url=' + encodeURIComponent(destination);
            return false;
          }
        },
        error: function (jqXHR, textStatus, errorThrown) {
          // Redirect to login.
          var destination = window.location;
          window.location = Drupal.recipeBox.shopSigninUrl + '?forward-url=' + encodeURIComponent(destination);
          return false;
        }
    });
  };

  /**
   * Activate remove button.
   */
  Drupal.recipeBox.activateRemoveButtons = function (recipeList) {
    $('.js-recipe-box-button-remove', recipeList).each(function (index) {
      $(this).on('click', function (e) {
        e.preventDefault();

        var uuid = $(this).data('recipe-uuid');

        $.ajax({
          type: "DELETE",
          url: Drupal.recipeBox.apiBaseUrl,
          data: { rid: uuid },
          xhrFields: { withCredentials: true },
          success: function (response) {
            if (response[0] == true) {
              $('[data-recipe-box-row-id="' + uuid + '"]', recipeList).remove();
            }
            else {
              console.error('activateRemoveButtons returned error: ' + errorThrown);
            }
          },
          error: function (response, textStatus, errorThrown) {
            console.error('activateRemoveButtons returned error: ' + errorThrown);
          }
        });
      })
    });
  };

  Drupal.behaviors.kafRecipeBoxView = {
    attach: function (context, settings) {
      if (!$('.js-recipe-box-container', context).length) {
        return;
      }

      // Fetch Recipe Box list on /recipebox.
      $.ajax({
        type: "GET",
        url: Drupal.recipeBox.apiBaseUrl,
        dataType: 'json',
        xhrFields: { withCredentials: true },
        success: function (response) {
          var recipeList = $(Drupal.theme('kafRecipeBoxList')).appendTo($('.js-recipe-box-container', context));
          Drupal.recipeBox.getRecipeBoxItems(response, recipeList);
        },
        error: function (jqXHR, textStatus, errorThrown) {
          if (jqXHR.state() == 'rejected') {
            // Redirect to login.
            var destination = window.location;
            window.location = Drupal.recipeBox.shopSigninUrl + '?forward-url=' + encodeURIComponent(destination);
            return false;
          }
        }
      });
    }
  };

  Drupal.behaviors.kafRecipeBoxAddButton = {
    attach: function (context, settings) {
      if (!$('.js-kaf-recipe-box-add', context).length) {
        return;
      }

      // Add Recipe button on recipe detail pages.
      $('.js-kaf-recipe-box-add', context).on('click', function (e){
        e.preventDefault();
        Drupal.recipeBox.addRecipeToBox(this, $(this).data('recipe-id'));
      });
    }
  };

})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal) {
  Drupal.behaviors.kafStatusMessageUI = {
    attach: function (context, settings) {
      // Really just to avoid unnecessarily running code;
      // context restriction for handlers seems unneeded.
      if (!$('#kaf-comment-submission-modal', context).length) {
