      var nonSslPage = 'http://' + currPage;
      var altPage = '';
      var fbToken = '&access_token=131155497576678|5aa026816e49608b3de70449cc552a1d';
      var fbFields = '&fields=engagement';
      var fbPrefix = 'https://graph.facebook.com/?id=';
      var pinPrefix = 'https://api.pinterest.com/v1/urls/count.json?url=';

      var externalApiRequests = {};
      externalApiRequests[fbPrefix + sslPage + fbToken + fbFields] = 'facebook';
      externalApiRequests[pinPrefix + sslPage + '&callback=?'] = 'pinterest';

      // Include alternate format URLs for guides, collections, and blog posts.
      if (sslPage.indexOf('/blog/') > 0) {
        altPage = sslPage.replace('/blog/', '/').replace('www.king', 'blog.king') + '/';
        externalApiRequests[fbPrefix + altPage + fbToken + fbFields] = 'facebook';
        externalApiRequests[pinPrefix + altPage + '&callback=?'] = 'pinterest';
      }
      else if (sslPage.indexOf('/collections/') > 0) {
        altPage = sslPage.replace('/recipes/collections/', '/collections/') + '/';
        externalApiRequests[fbPrefix + altPage + fbToken + fbFields] = 'facebook';
        externalApiRequests[pinPrefix + altPage + '&callback=?'] = 'pinterest';
      }
      else if (sslPage.indexOf('/guides/') > 0) {
        altPage = sslPage.replace('/learn/guides/', '/guides/') + '/';
        externalApiRequests[fbPrefix + altPage + fbToken + fbFields] = 'facebook';
        externalApiRequests[pinPrefix + altPage + '&callback=?'] = 'pinterest';
      }
      else if (/\/recipes\/.*-recipe$/.test(sslPage)) {
        // Include non-SSL Pinterest for recipes.
        externalApiRequests[pinPrefix + nonSslPage + '&callback=?'] = 'pinterest';
      }

      var shareTotal = 0;
      var asyncCounter = 0;
      $.each(externalApiRequests, function (url, type) {
        $.ajax(url, {
          dataType: 'json'
        }).done(function (data) {
          // This is going to run asynchronously no matter what.
          if (type == 'facebook') {
            var count = parseInt(
              data.engagement.share_count + data.engagement.reaction_count + data.engagement.comment_count,
              10
            );
            shareTotal += count;
          }
          else if (type == 'pinterest') {
            shareTotal += parseInt(data.count, 10);
          }

          asyncCounter++;
          if (asyncCounter == Object.keys(externalApiRequests).length) {
            // This will only run once.
            self.displayShareTotal(shareTotal);
          }
        });
      });
    },

    displayShareTotal: function (num) {
      if (num > 0) {
        var formattedNum = num > 999 ? (num / 1000).toFixed(1) + 'k' : num;
        $('.js-kaf-share-widget').removeClass('no-shares');
        $('.js-kaf-share-count').removeClass('hidden').html(formattedNum);
      }
    },

    activateShareButtons: function (container, thisPageTitle, thisPageUrl) {
      var self = this;

      var facebookShare = 'https://www.facebook.com/dialog/share?app_id=131155497576678&href=' + thisPageUrl;
      var twitterShare = 'https://twitter.com/intent/tweet?text=' + thisPageTitle + '%20' + thisPageUrl;
      var emailShare = 'mailto:?subject=' + thisPageTitle + '&body=' + thisPageTitle + '%0A' + thisPageUrl;

      $('.js-kaf-share-button-pinterest', container).on('click', function (e) {
        e.preventDefault();
        self.showPinnableImages(thisPageTitle, thisPageUrl);
      });

      $('.js-kaf-share-button-facebook', container).on('click', function (e) {
        e.preventDefault();
        window.open(facebookShare, 'window', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      });

      // Update href attribute instead of opening a new window
      $('.js-kaf-share-button-email').attr('href', emailShare);

      $('.js-kaf-share-button-twitter', container).on('click', function (e) {
        e.preventDefault();
        window.open(twitterShare, 'window', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      });

      // Allow spacebar to activate "buttons" as if they were really buttons.
      $('a', container).on('keydown', function (e) {
        if (e.which === 32) {
          e.preventDefault();
          $(this).click();
        }
      });
    },

    showPinnableImages: function (thisPageTitle, thisPageUrl) {
      var self = this;

      var rawMeta = $('meta[name="description"]').attr('content');
      var thisPageMeta = '';
      if (rawMeta && rawMeta.length > 0) {
        thisPageMeta = encodeURIComponent(': ' + rawMeta);
      }
      var images = $(
        '.quickview .quickview__media img, .node__hero img, .article__body img[data-pin-nopin!="true"][nopin!="nopin"]'
      );

      var overlayOpen =
        '<div class="pinterest-overlay js-pinterest-overlay" onclick="void(0)">' +
        '<div class="pin-overlay-close"><a><i class="icon-close--bold">&nbsp;</i></a></div>' +
        '<h1><span><i class="icon-pinterest">&nbsp;</i></span> Choose a Pin to save</h1>';
      var overlayClose = '</div>';

      var imageCount = 0;
      var imageLinks = [];
      var imageText = [];
      images.each(function () {
        if (
          $(this).attr('data-pin-media') ||
          ($(this).attr('class') && $(this).attr('class').indexOf('pinterest') !== -1)
        ) {
          // special handling for special pinterest images
          var src = '';
          if ($(this).attr('data-pin-media')) {
            src = $(this).attr('data-pin-media');
          }
          else if ($(this).attr('src')) {
            src = $(this).attr('src');
          }
          else {
            src = $(this).attr('srcset');
          }

          // Convert the image src to absolute.
          src = Drupal.url.toAbsolute(src);
          imageLinks.push(src);

          if ($(this).attr('alt')) {
            imageText.push($(this).attr('alt'));
          }
          else {
            imageText.push(thisPageTitle + thisPageMeta);
          }

          imageCount++;
        }
        else if ($(this)[0].width >= 236) {
          var src = '';
          if ($(this).attr('src')) {
            src = $(this).attr('src');
          }
          else {
            src = $(this).attr('srcset');
          }

          // Convert the image src to absolute.
          src = Drupal.url.toAbsolute(src);
          imageLinks.push(src);

          imageText.push(thisPageTitle + thisPageMeta);
          imageCount++;
        }
      });

      if (imageCount > 1) {
        var overlay = overlayOpen;
        // construct overlay contents from arrays of links and text
        for (var i = 0; i < imageCount; i++) {
          overlay +=
            '<div class="pinterest-option"><a><img src="' + imageLinks[i] + '" alt="' + imageText[i] + '"></a></div>';
        }
        overlay += overlayClose;
        $('body').append(overlay);

        $('.js-pinterest-overlay').on('click', function (e) {
          if ($(e.target).closest('.pinterest-option').length === 0) {
            $('.js-pinterest-overlay').remove();
          }
          else {
            var pin = $(e.target).closest('.pinterest-option');
            window.open(
              'https://www.pinterest.com/pin/create/button/?url=' +
                thisPageUrl +
                '&description=' +
                pin.find('img').attr('alt') +
                '&media=' +
                pin.find('img').attr('src'),
              'window',
              'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600'
            );
            $('.js-pinterest-overlay').remove();
          }
        });
      }
      else if (imageCount === 1) {
        window.open(
          'https://www.pinterest.com/pin/create/button/?url=' +
            thisPageUrl +
            '&description=' +
            imageText[0] +
            '&media=' +
            imageLinks[0],
          'window',
          'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600'
        );
      }
      else if (thisPageUrl.includes('.com%2Frecipes%2F')) {
        // recipe page with no images? use main recipe pin image
        window.open(
          'https://www.pinterest.com/pin/create/button/?url=' +
            thisPageUrl +
            '&description=' +
            thisPageTitle +
            thisPageMeta +
            '&media=https%3A%2F%2Fwww.kingarthurflour.com%2Fimages%2Fsharing%2Frecipe-home-jan2016-pinterest.jpg',
          'window',
          'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600'
        );
      }
    }
  };
})(jQuery, Drupal);
